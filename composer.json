{
  "name": "csg/phoboslab-pagenode",
  "description": "A project base skeleton using pagenode [https://pagenode.org/] [https://github.com/phoboslab/pagenode].",
  "require": {
    "ext-intl": "*",
    "erusev/parsedown": "^1.7",
    "symfony/console": "^6.0",
    "symfony/filesystem": "^6.4",
    "symfony/mailer": "^6.0",
    "symfony/yaml": "^6.0"
  },
  "require-dev": {
    "andres-montanez/magallanes": "^5.0",
    "doctrine/dbal": "^4.0",
    "symfony/var-dumper": "^6.0"
  },
  "authors": [
    {
      "name": "Christian Sonntag",
      "email": "info@motions-media.de"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "Pagenode\\Importer\\": "src/Pagenode/Importer"
    }
  }
}
